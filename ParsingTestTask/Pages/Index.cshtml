@page
@model ParsingTestTask.Pages.IndexModel
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <title>Offer</title>
</head>

<div class="container mt-5 text-center">
    @*<img src=@Model.offer.Picture class="img-rounded" alt="Pink Floyd">*@
    <table class="table table-striped">
        <tbody>
            <tr>
                <td>Категория</td>
                <td>@Model.offer.Category</td>
            </tr>
            <tr>
                <td>Id категории</td>
                <td>@Model.offer.CategoryId</td>
            </tr>
            <tr>
                <td>Валюта</td>
                <td>@Model.offer.CurrencyId</td>
            </tr>
            <tr>
                <td>Цена</td>
                <td>@Model.offer.Price</td>
            </tr>
            <tr>
                <td>Ссылка на картинку</td>
                <td>@Model.offer.Picture</td>
            </tr>
            <tr>
                <td>Доставка</td>
                <td>@Model.offer.Delivery</td>
            </tr>
            <tr>
                <td>Исполнитель</td>
                <td>@Model.offer.Artist</td>
            </tr>
            <tr>
                <td>Описание</td>
                <td>@Model.offer.Description</td>
            </tr>
            <tr>
                <td>Ссылка</td>
                <td>@Model.offer.Url</td>
            </tr>
            <tr>
                <td>Медиа</td>
                <td>@Model.offer.Media</td>
            </tr>
            <tr>
                <td>Год</td>
                <td>@Model.offer.Year</td>
            </tr>
        </tbody>
    </table>
    <img />
</div>




